{
  "productName": "Tashgheel Restaurants",
  "version": "1.0.1",
  "identifier": "com.tashgeel-restaurants.pos",
  "build": {
    "beforeDevCommand": "npm run serve",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:1421",
    "frontendDist": "../"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Tashgheel Restaurants",
        "width": 1280,
        "height": 800,
        "fullscreen": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}